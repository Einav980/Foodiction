<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FoodictionPinkTheme" parent="">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/lightOrangePink</item>
        <item name="colorPrimaryVariant">@color/bronzer</item>
        <item name="colorOnPrimary">@color/offWhite</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/darkPink</item>
        <item name="colorSecondaryVariant">@color/lightPink</item>
        <item name="colorOnSecondary">@color/bronzer</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

</resources>